{
    "name": "umutphp/laravel_model_recommendation",
    "description": "A generic package to create recommendation for eloquent models",
    "keywords": [
        "umutphp",
        "laravel",
        "laravel_model_recommendation"
    ],
    "homepage": "https://github.com/umutphp/laravel_model_recommendation",
    "license": "MIT",
    "authors": [
        {
            "name": "Umut Işık",
            "email": "umutphp@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.4.3",
        "illuminate/contracts": "^8.0"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.9",
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "Umutphp\\LaravelModelRecommendation\\": "src",
            "Umutphp\\LaravelModelRecommendation\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Umutphp\\LaravelModelRecommendation\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Umutphp\\LaravelModelRecommendation\\LaravelModelRecommendationServiceProvider"
            ],
            "aliases": {
                "LaravelModelRecommendation": "Umutphp\\LaravelModelRecommendation\\LaravelModelRecommendationFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
